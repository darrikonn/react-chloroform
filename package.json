{
  "name": "react-chloroform",
  "version": "0.1.3",
  "description": "A framework for building forms in React applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darrikonn/react-chloroform.git"
  },
  "keywords": [
    "form",
    "form-validation",
    "react",
    "react-chloroform",
    "reactjs"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "author": "Darri Steinn Konradsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darrikonn/react-chloroform/issues"
  },
  "homepage": "https://github.com/darrikonn/react-chloroform#readme",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc-watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "tsc": "tsc --noEmit",
    "format": "prettier --config .prettierrc --write \"src/**/*.ts*\"",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.9.19",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.7.4",
    "tsc-watch": "^4.1.0",
    "typescript": "^3.7.5"
  }
