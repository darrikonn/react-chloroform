{
  "presets": ["react", ["es2015", {"modules": false}], "stage-0"],
  "env": {
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }]
      ],
    },
    "es": {
      "plugins": [
        "./build/babel-lodash-es",
        "transform-class-properties"
      ],
    }
  }
}
